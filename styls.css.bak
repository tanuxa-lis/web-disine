html {
	padding: 40px;
	background: -webkit-linear-gradient(#FFFF00, #FF8C00);
}
body{
	border: 3pt dashed #800000;
	padding: 40px;
	box-shadow: 0 0 40pt rgba(0,0,0,0.9);
	background-color: #F0E68C;
	background-image: url(./images/16k.avif);
}
p{
  font-family: "Sevillana", cursive;
  font-weight: 400;
  font-style: normal;
  color: #191970;
  font-size: 16pt;
}
h1, h2 {
	color: #800080;
}